ul {
    list-style: none;
    margin: 0;
    padding: 0;
  }
li {
  display: inline-block;
}
.container {
  display: block;
  max-width: 960px;
  margin: 0 auto;
}
.home-menu,
.home-menu a {
  color: #fff;
}
.home-menu {
  margin-top: 240px;
}
.home-menu h1 {
  font-size: 72px;
  font-family: 'Poiret One', cursive; 
  text-shadow: 1px 1px 1px rgba(0,0,0,0.3);
}
.home-menu li {
  margin: 10px 20px;
  font-size: 24px;
  text-shadow: 1px 1px 1px rgba(0,0,0,0.3);
}
.social-links {
  margin-top: 20px;
}
.home-menu .icon--ei-sc-instagram {
  fill: #f2f2f2;
}
.nav h3 {
  margin: 20px;
  font-size: 36px;
  font-family: 'Poiret One', cursive;
}
.nav ul {
  border-top: 1px solid #999;
  border-bottom: 1px solid #999;
  margin: 0 auto;
  max-width: 940px;
  overflow-x: scroll;
  overflow-y: hidden;
  white-space: nowrap;
  word-wrap: normal;
}
.nav li {
  margin: 10px 20px;
}
.nav-menu {
  overflow-x: scroll;
  overflow-y: hidden;
  white-space: nowrap;
  word-wrap: normal;
}
.nav-menu li {
  margin: 10px;
}
.nav-menu img {
  max-width: 50px;
}
.entry-container {
  margin: 0 auto;
}
.entry {
  float: left;
  max-width: 300px;
  height: 300px;
  margin: 10px;
  padding: 0;
  position: relative;
}
.entry:hover,
.entry:active {
  opacity: 0.90;
  filter: alpha(opacity=90);
}
.entry img {
  max-width: 100%;
}
.entry h2 {
  margin: -290px 0 0 0;
  padding: 0 10px 0 10px;
  font-size: 24px;
  font-family: inherit;
  font-weight: 400;
  line-height: 26px;
  color: #ffffff;
  text-transform: uppercase;
  text-align: left;
  text-shadow: 1px 1px 1px rgba(0,0,0,0.3);
  text-rendering: optimizelegibility;
}
.entry h4 {
  margin: 5px 0;
  padding: 0 10px 0 10px;
  font-size: 16px;
  font-family: inherit;
  font-weight: 200;
  line-height: 26px;
  color: #ffffff;
  text-transform: uppercase;
  text-align: left;
  text-shadow: 1px 1px 1px rgba(0,0,0,0.3);
  text-rendering: optimizelegibility;
}
.entry-price {
  margin: 5px 0;
  padding: 0 10px 0 10px;
  font-size: 16px;
  font-family: inherit;
  font-weight: 400;
  line-height: 26px;
  color: #ffffff;
  text-transform: uppercase;
  text-align: left;
  text-shadow: 1px 1px 1px rgba(0,0,0,0.3);
  text-rendering: optimizelegibility;
}
.info {
  margin: 20px;
}
.center {
  text-align: center;
}
.clearfix {
    clear: both;
    height:0;
    visibility:hidden
}
.middle {
  vertical-align: middle;
}

@media (min-width: 960px) and (max-width: 980px) {
  .entry-container {
    max-width: 960px;
  }
}
@media (min-width: 640px) and (max-width: 959px) {
  .entry-container {
    max-width: 640px;
  }
}
@media (max-width: 639px) {
  .entry-container {
    max-width: 300px;
  }
  .entry {
    margin: 5px 0;
  }
  .home-menu {
    margin-top: 20px; 
  }
  .home-menu h1 {
    font-size: 36px;
  }
}